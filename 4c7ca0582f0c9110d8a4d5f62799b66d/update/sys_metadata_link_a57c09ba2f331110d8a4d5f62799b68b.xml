        &lt;name&gt;AgileMultiTaskColumnsAndFieldServiceBase&lt;/name&gt;
        &lt;script&gt;&lt;![CDATA[var AgileMultiTaskColumnsAndFieldServiceBase = Class.create();

AgileMultiTaskColumnsAndFieldServiceBase.columns = [
{
	name: 'original_task',
	label: gs.getMessage('Number'),
	column: sn_ui.PolarisUI.isEnabled() ? 3 : 2,
	display: '&lt;a ng-href="{{item[\'original_task.sys_class_name\'].value || item.sys_class_name.value}}.do?sys_id={{item[\'original_task.sys_id\'].value || item.sys_id}}" ng-click="editRecord($event,item)"&gt;{{item[column.name].display_value}}&lt;/a&gt;'
},{
    name: 'original_task.short_description',
	column: 8,
    'class': 'text-ellipsis',
    label: gs.getMessage('Short Description'),
    display: '&lt;span tooltip-append-to-body="true" uib-tooltip="{{item[column.name].value}}" tooltip-popup-delay="200"&gt;{{item[column.name].value}}&lt;/span&gt;'
}];

AgileMultiTaskColumnsAndFieldServiceBase.mandatoryFields = "original_task.number,original_task.sys_class_name,original_task.sys_id";
AgileMultiTaskColumnsAndFieldServiceBase.iconSrc = "{{iconMap[item['original_task.sys_class_name'].value] || iconMap['default']}}";

AgileMultiTaskColumnsAndFieldServiceBase.getColumnsAndFields = function() {
	return {
		columns: AgileMultiTaskColumnsAndFieldServiceBase.columns,
		mandatoryFields: AgileMultiTaskColumnsAndFieldServiceBase.mandatoryFields,
		iconSrc: AgileMultiTaskColumnsAndFieldServiceBase.iconSrc
	};
};]]&gt;&lt;/script&gt;
